    </r:Description>
  </pr:Used>
  <pr:Used defaultValue="DDI Mode of Collection" fixedValue="true" isRequired="false" xpath="/codeBook/stdyDscr/method/dataColl/collMode/concept/@vocab">
    <r:Description>
      <r:Content>Required: Recommended</r:Content>
      <r:Content>ElementType: Attribute</r:Content>
